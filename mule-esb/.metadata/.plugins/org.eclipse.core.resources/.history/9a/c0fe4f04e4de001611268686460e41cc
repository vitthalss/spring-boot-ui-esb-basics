        <logger message="#[payload]" level="INFO" doc:name="Logger"/>
        <json:xml-to-json-transformer doc:name="XML to JSON"/>
        <logger message="#[payload]" level="INFO" doc:name="Logger"/>
        <file:outbound-endpoint path="/home/xcaliber/Vitthal/xcaliber documents/FTP/Output" responseTimeout="10000" doc:name="File"/>
    </flow>
